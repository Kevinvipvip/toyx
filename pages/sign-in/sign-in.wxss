.sign-in-top {
    height: 668rpx;
    background-image: linear-gradient(180deg, #fbb03b 0%, #fb833b 100%);
    display: flex;
    justify-content: center;
    align-items: center;
    position: relative;
}

.sign-in-top-text {
    position: absolute;
    top: 40rpx;
    left: 30rpx;
    right: 30rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.sign-in-top-text .top-text {
    font-size: 26rpx;
    color: #f5f4f4;
    display: flex;
    text-align: center;
}

.sign-in-top-text .top-text image {
    width: 26rpx;
    height: 26rpx;
    display: block;
    margin-right: 8rpx;
    margin-top: 5rpx;
}

