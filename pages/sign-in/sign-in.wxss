.sign-in-top .every-day {
    position: absolute;
    left: 40rpx;
    right: 40rpx;
    bottom: 60rpx;
    display: flex;
    justify-content: space-around;
}

.sign-in-top .day-item {
    width: 14.285%;
    opacity: 0.5;
}

.sign-in-top .day-item text {
    color: #ffffff;
    font-size: 24rpx;
    display: block;
    text-align: center;
}

.sign-in-top .day-item .item-top {
    height: 50rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    position: relative;
}

.sign-in-top .day-item .item-top .spot {
    position: relative;
    z-index: 2;
    width: 16rpx;
    height: 16rpx;
    background-color: #f7f7f7;
    border-radius: 50%;
    display: flex;
    justify-content: center;
    align-items: center;
}

.sign-in-top .day-item .item-top .spot text {
    display: block;
    width: 50%;
    height: 50%;
    border-radius: 50%;
    background-color: #ff6d15;
}

.sign-in-top .day-item .item-top .line {
    position: absolute;
    z-index: 1;
    width: 100%;
    height: 4rpx;
    background-color: #f7f7f7;
}

.sign-in-top .day-item:first-child .item-top .line {
    width: 50%;
    right: 0;
}

.sign-in-top .day-item:last-child .item-top .line {
    width: 50%;
    left: 0;
}

/* 签到成功后进度条样式 */
.sign-in-top .on {
    opacity: 1;
}

/* 签到按钮样式 */
.sign-in-top .sign-in-button .btn-sign-in {
    width: 260rpx;
    height: 260rpx;
    background-image: url("data:image/png;base64,");
    background-position: center;
    background-repeat: no-repeat;
    background-size: 100%;
    box-shadow: 0 20rpx 20rpx 0 rgba(247, 130, 55, 0.4);
    display: flex;
    flex-flow: column;
    justify-content: center;
    align-items: center;
    border-radius: 50%;
    font-size: 48rpx;
    color: #ff6d15;
}

